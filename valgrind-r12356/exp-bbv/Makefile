	$(CC) $(DEFS) $(DEFAULT_INCLUDES) $(INCLUDES) $(exp_bbv_x86_linux_CPPFLAGS) $(CPPFLAGS) $(exp_bbv_x86_linux_CFLAGS) $(CFLAGS) -MT exp_bbv_x86_linux-bbv_main.obj -MD -MP -MF $(DEPDIR)/exp_bbv_x86_linux-bbv_main.Tpo -c -o exp_bbv_x86_linux-bbv_main.obj `if test -f 'bbv_main.c'; then $(CYGPATH_W) 'bbv_main.c'; else $(CYGPATH_W) '$(srcdir)/bbv_main.c'; fi`
	$(am__mv) $(DEPDIR)/exp_bbv_x86_linux-bbv_main.Tpo $(DEPDIR)/exp_bbv_x86_linux-bbv_main.Po
