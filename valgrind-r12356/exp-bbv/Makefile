.c.o:
	$(COMPILE) -MT $@ -MD -MP -MF $(DEPDIR)/$*.Tpo -c -o $@ $<
	$(am__mv) $(DEPDIR)/$*.Tpo $(DEPDIR)/$*.Po
#	source='$<' object='$@' libtool=no \
#	DEPDIR=$(DEPDIR) $(CCDEPMODE) $(depcomp) \
#	$(COMPILE) -c $<

.c.obj:
	$(COMPILE) -MT $@ -MD -MP -MF $(DEPDIR)/$*.Tpo -c -o $@ `$(CYGPATH_W) '$<'`
	$(am__mv) $(DEPDIR)/$*.Tpo $(DEPDIR)/$*.Po
#	source='$<' object='$@' libtool=no \
#	DEPDIR=$(DEPDIR) $(CCDEPMODE) $(depcomp) \
#	$(COMPILE) -c `$(CYGPATH_W) '$<'`

exp_bbv_x86_linux-bbv_main.o: bbv_main.c
	$(CC) $(DEFS) $(DEFAULT_INCLUDES) $(INCLUDES) $(exp_bbv_x86_linux_CPPFLAGS) $(CPPFLAGS) $(exp_bbv_x86_linux_CFLAGS) $(CFLAGS) -MT exp_bbv_x86_linux-bbv_main.o -MD -MP -MF $(DEPDIR)/exp_bbv_x86_linux-bbv_main.Tpo -c -o exp_bbv_x86_linux-bbv_main.o `test -f 'bbv_main.c' || echo '$(srcdir)/'`bbv_main.c
	$(am__mv) $(DEPDIR)/exp_bbv_x86_linux-bbv_main.Tpo $(DEPDIR)/exp_bbv_x86_linux-bbv_main.Po
#	source='bbv_main.c' object='exp_bbv_x86_linux-bbv_main.o' libtool=no \
