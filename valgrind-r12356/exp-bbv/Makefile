#	$(CC) $(DEFS) $(DEFAULT_INCLUDES) $(INCLUDES) $(exp_bbv_x86_linux_CPPFLAGS) $(CPPFLAGS) $(exp_bbv_x86_linux_CFLAGS) $(CFLAGS) -c -o exp_bbv_x86_linux-bbv_main.o `test -f 'bbv_main.c' || echo '$(srcdir)/'`bbv_main.c

